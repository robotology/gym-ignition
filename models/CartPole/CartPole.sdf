    <link name='cart'>
      <pose frame=''>0 0 0.5 0 -0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.00354167</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00416667</iyy>
          <iyz>0</iyz>
          <izz>0.00104167</izz>
        </inertia>
      </inertial>
      <visual name='cart_visual'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.2 0.05</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 0.8</ambient>
          <diffuse>0.2 0.2 0.2 0.8</diffuse>
          <specular>0.2 0.2 0.2 0.8</specular>
