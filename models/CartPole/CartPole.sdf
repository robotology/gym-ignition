    <joint name='linear' type='prismatic'>
      <child>cart</child>
      <parent>rail</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-2.4</lower>
          <upper>2.4</upper>
          <effort>500</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
    <link name='pole'>
      <pose frame=''>0 0 0.525 0 -0 0</pose>
      <inertial>
        <pose frame=''>0 0 0.5 0 -0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>1e-09</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0333333</iyy>
          <iyz>0</iyz>
          <izz>0.0333333</izz>
